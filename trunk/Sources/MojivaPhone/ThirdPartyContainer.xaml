<UserControl x:Class="MojivaPhone.ThirdPartyContainer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:MMAdView="clr-namespace:mmiWP7SDK;assembly=WP7SDK"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
	d:DesignHeight="800" d:DesignWidth="480">

	<StackPanel x:Name="LayoutRoot" HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Width="480" Background="#249D9B9B" Height="800" Loaded="LayoutRoot_Loaded" IsHitTestVisible="False">
		<MMAdView:MMAdView Name="mMAdView" Apid="0" Width="0" Height="0" RefreshTimer="-1" AdType="MMBannerAdTop" />
	</StackPanel>
</UserControl>
